			}
		};
 
		
		var applicationImage = '';
		
		switch (appName) {
		
			case "MyFleetOffice":
				applicationImage = "mfo_wht.png";
			break;
				
			case "MyFleetOffice2":
				applicationImage = "mfo2_wht.png";
			break;
			
			case "MyVehicle":
				applicationImage = "myVehicle_allwht.png";
			break;
			
			default:	
				applicationImage = "mfo_wht.png";
		}
 
		
		sessionStorage.setItem("applicationImage", applicationImage);
 
	
