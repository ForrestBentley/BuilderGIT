<script type="text/javascript">
	//
		$(document).ready(function(){
			if (top.frames.length!=0){
				top.location=self.document.location;
			}
			var bodyContentClass = 'bodycontent3';
			
			function getRandomNumber(start, end){
				var lowerLimit = start;
				var upperLimit = end;
				var imageNum = '';
				
				imageNum = Math.floor(Math.random() * upperLimit) + lowerLimit;
				
				return imageNum;
				
			};
 
			
			switch(getRandomNumber(1,5))
			{
				case 1:
					bodyContentClass = 'bodycontent1';
				break;
 
				case 2:
					bodyContentClass = 'bodycontent2';
 
				case 3:
					bodyContentClass = 'bodycontent3';					
 
				break;
 
				case 4:
					bodyContentClass = 'bodycontent4';
					
				break;
				
				default:
					bodyContentClass = 'bodycontent2';
			}
			
			$('#login').removeClass('bodycontent3').addClass(bodyContentClass);
			
			var appName = $.cookie('APPLICATION_NAME');
 
//			$.cookie("Pgact", "driverportal", {path:'/'});
			
			// check for Pgact cookie to see if this is driverportal. If it exists see if it is for driverportal in any environment
			if ($.cookie('Pgact')) {
				if ($.cookie('Pgact').indexOf('driverportal') >= 0) {
					appName = "MyVehicle";
					$('.leftcolumn_spacer').css('display', 'none');
				}
			};
			
 
			//console.log("appname", appName);			
			
			var applicationImage = '';
			
			switch (appName) {
			
				case "MyFleetOffice":
					applicationImage = "mfo_wht.png";
				break;
					
				case "MyFleetOffice2":
					applicationImage = "mfo2_wht.png";
				break;
				
				case "MyVehicle":
					applicationImage = "myVehicle_allwht.png";
				break;
				
				default:	
					applicationImage = "mfo_wht.png";
			}
			
			sessionStorage.setItem("applicationImage", applicationImage);
			
			
			sessionStorage.setItem("backgroundCSS", bodyContentClass);
			imageUrl = sessionStorage.getItem("applicationImage");
			$('#logo').css('background-image', 'url("resources/web-resources/img/' + imageUrl + '")');
			
		});
	//
</script>
